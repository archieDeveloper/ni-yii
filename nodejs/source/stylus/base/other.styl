.copyright
  font-size: 10px;
  font-weight: bold;
  text-transform: uppercase;
  text-align: center;
  color: #aaa;
  margin: 20px 0;

.big-circle-image
  width 250px
  height 250px
  overflow hidden
  border-radius 200px
  float left
  margin-right 30px
  border 5px solid #FFFFFF
  box-shadow 0 5px 0 rgba(0,0,0,0.1)
  & img
    width 100%

.callback
  float right
  width 300px

.float-left
  float left
  margin-bottom 20px

.float-right
  float right
  margin-bottom 10px